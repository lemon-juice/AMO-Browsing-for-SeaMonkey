<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <rdf:Description rdf:about="urn:mozilla:install-manifest">
    <em:id>amobrowsing@seamonkey.mozilla.org</em:id>
    <em:version>0.10.3</em:version>
    <em:type>2</em:type>
    <em:bootstrap>true</em:bootstrap>

    <em:targetApplication>
      <!-- SeaMonkey -->
      <Description>
        <em:id>{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}</em:id>
        <em:minVersion>2.41</em:minVersion>
        <em:maxVersion>2.53.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <em:targetApplication>
      <!-- Firefox Developer Edition (use only for testing, remove before submitting to AMO) -->
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>44.0</em:minVersion>
        <em:maxVersion>*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <em:targetApplication>
      <!-- Pale Moon (not sure how helpful this is, but it works) -->
      <Description>
        <em:id>{8de7fcbb-c55c-4fbe-bfc5-fc555c87dbc4}</em:id>
        <em:minVersion>27.0</em:minVersion>
        <em:maxVersion>*</em:maxVersion>
      </Description>
    </em:targetApplication>
 
   <em:name>AMO Browsing for SeaMonkey</em:name>
    <em:description>This extension makes browsing for extensions easier at addons.mozilla.org and addons.thunderbird.net. It fixes incorrect information about some extensions being not available for SeaMonkey and adds links to the Add-on Converter where needed.
    </em:description>
    <em:creator>Isaac Schemm; Lemon Juice</em:creator>
    <em:homepageURL>https://github.com/lemon-juice/AMO-Browsing-for-SeaMonkey</em:homepageURL>
  </rdf:Description>
</rdf:RDF>
